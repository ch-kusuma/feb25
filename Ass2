n=int(input())
l=[]
t=[]
for i in range(n):
    x=int(input())
    l.append(x)
c=0    
for i in range(n):
    if l[i]!=0:
     t.append(l[i])
     c=c+1
for i in range(n):     
    if l[i]==0:
      t.append(l[i])   
      t.remove(l[i])  
print(c)     
print(t)        
    


